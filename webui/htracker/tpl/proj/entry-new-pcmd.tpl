<div class="htracker-modal-container">
<div id="htracker-projset-pcmd-alert" class="alert less-hide"></div>

<div class="form-group row">
  <label class="col-sm-3 col-form-label">{[=l4i.T("Project Name")]}</label>
  <div class="col-sm-9">
    <input type="text" class="form-control inputfocus" id="htracker_projset_name" value="{[=it.name]}">
  </div>
</div>

<div class="form-group row">
  <label class="col-sm-3 col-form-label">{[=l4i.T("Command Match String")]}</label>
  <div class="col-sm-9">
    <input type="text" class="form-control" id="htracker_projset_proc_cmd" value="{[=it.filter.proc_cmd]}">
  </div>
</div>
</div>
